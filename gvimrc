source $HOME/.vimrc

set guifont=Monaco:h10
set noantialias

" no toolbar
set guioptions-=T

" no right and left scrollbars
set guioptions-=r
set guioptions-=L

set colorcolumn=""

" color rootwater
" color borland
" color inkpot
" color dusk
" color autumnleaf
" color candycode
" color synic
color lucius

set transparency=1
